@model IEnumerable<Results>

<h3>Race Results</h3>

<table class="table">
    <tr>
        <th>Finish Pos</th>
        <th>Car #</th>
        <th>Driver Name</th>
        <th>Start Pos</th>
        <th>Interval</th>
        <th>Laps Led</th>
        <th>Laps Comp</th>
        <th>Best Lap</th>
        <th>Best Lap Time</th>
        <th>Average Lap</th>
        <th>Inc</th>
        <th>Div</th>
        <th>Club</th>
        <th>SafetyRating</th>
        <th>iRating</th>
    </tr>
    
    @foreach(var result in Model)
    {
        <tr>
            <td>@result.FinishPosition</td>
            <td>@result.CarNumber</td>
            <td>@result.DisplayName</td>
            <td>@result.StartingPosition</td>
            <td>@result.FinishInterval</td>
            <td>@result.LapsLed</td>
            <td>@result.LapsComplete</td>
            <td>@result.BestLapNum</td>
            <td>@result.BestLapTime</td>
            <td>@result.AverageLap</td>
            <td>@result.Incidents</td>
            <td>@result.Division</td>
            <td>@result.ClubShortName</td>
            <td>@result.NewLicenseLevel @result.NewSafetyRating</td>
            <td>@result.NewiRating</td>
        </tr>
    }

</table>
